<params group="performancedata">
	<param name="pack_css" type="radio" default="1" label="Enable css packing" tooltip="inline" description="CSS Packing (otherwise refered to as CSS Concatination) refers to the combining of multiple files into a single file. This boosts your sites performance as fewer http requests are needed.">
		<option value="1">Yes</option>
		<option value="0">No</option>
	</param>
	<param name="pack_js" type="radio" default="1" label="Enable js packing" tooltip="inline" description="JS Packing (otherwise refered to as JS Concatination) refers to the combining of multiple files into a single file. This boosts your sites performance as fewer http requests are needed.">
		<option value="1">Yes</option>
		<option value="0">No</option>
	</param>
	<param name="gzip_compression" type="radio" default="1" label="Enable gzip" tooltip="inline" description="Gzip is a form of optimization that causes pages to be compressed before being transmitted from server to browser, making the transmitted data smaller, thus speeding up the delivery.">
		<option value="1">Yes</option>
		<option value="0">No</option>
	</param>
	<param name="load_mootools" type="radio" default="0" label="Enable mootools" tooltip="inline" description="Mootools is a javascript framework that is used mostly in the backend of Joomla! There are a few instances in the frontend where it is needed (which we have taken into account), but in most cases this is not needed and in order to get optimal performance we have completely removed it from the frontend. This saves around 75kb from your sites total weight.">
		<option value="1">Yes</option>
		<option value="0">No</option>
	</param>
	<param name="load_caption" type="radio" default="0" label="Enable caption.js" tooltip="inline" description="Like the Mootools explaination above (hover over the Mootools tooltip), the caption.js is a file that is loaded by default in Joomla! 1.5, irrespective of whether it is actually being used. We have removed this from loading to improve the overall performance of your site.">
		<option value="1">Yes</option>
		<option value="0">No</option>
	</param>
</params>

<params group="debuggingdata">
	<param name="code_comments" type="radio" default="0" label="Enable code comments" tooltip="inline" description="Morph's code comments are currently in their infancy, but will be extensively improved before the stable release. The idea behind them is to make it super easy to find exactly where a section of code is being loaded from. Each comment will wrap around the specific section of code, include the filename in which it resides as well as the line number.">
		<option value="1">Yes</option>
		<option value="0">No</option>
	</param>
	<param name="error_reporting" type="radio" default="0" label="Enable error reporting" tooltip="inline" description="This will enable all php errors and notices to be displayed.">
		<option value="1">Yes</option>
		<option value="0">No</option>
	</param>
	<param name="jquery_core" type="radio" default="1" label="Enable jQuery" tooltip="inline" description="Morph uses jQuery extensively, so this should generally be left enabled. We have added the option to disable more for testing / debugging purposes.">
		<option value="1">Yes</option>
		<option value="0">No</option>
	</param>
	<param name="debug_modules" type="radio" default="0" label="Debug Modules" tooltip="inline" description="Enabling this option will globally override all module chromes to use the outline chrome option. This is good when you want to see which module positions are being used where.">
